          <Control />
          <Label text="成都天狐威视有限公司 版权所有" align="vcenter" font="3" />
          <Label text="http://www.tingfox.com" align="vcenter" font="3" />
          <Control />
          <Label text="特别声明：" align="vcenter" font="3" />
          <Label text="本软件为免费软件，由大学生创业团队完成。" align="vcenter" font="3" />
          <Label text="不喜勿喷！" align="vcenter" font="3" />
          <Control />
        </VerticalLayout>
        <!-- 帮助页面 End -->
      </TabLayout>
      <Label height="1" bkcolor="#" />
      <Label height="1" bkcolor="#FF444444" />
      <Label height="1" bkcolor="#FF666666" />
      <Label height="1" bkcolor="#" />
    </VerticalLayout>
  </VerticalLayout>
</Window>
