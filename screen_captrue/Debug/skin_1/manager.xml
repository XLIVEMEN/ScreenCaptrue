<?xml version="1.0" encoding="utf-8"?>
<Window size="570,320" caption="0,0,0,30">
  <Font name="Arial" size="14" bold="false" italic="false" default="true" />
  <Font name="Arial" size="14" bold="true" italic="false" default="false" />
  <Font name="Arial" size="18" bold="false" italic="false" default="false" />
  <Font name="Arial" size="22" bold="false" italic="false" default="false" />
  <Include source="default.xml" />
  <StringTable xmlfile="string_table.xml" />
  <VerticalLayout bkcolor="#ff35383f">
    <HorizontalLayout height="30" bkcolor="#ffcd364b" inset="5,1,3,0">
      <Label name="caption" width="40" text="#system_name" font="2" textcolor="#ff000000" align="vcenter" />
      <Control />
      <HorizontalLayout width="70" bkcolor="#ff35383f" inset="0,6,5,7">
        <Control />
        <Button name="minbtn" tooltip="#minimize" width="16" height="16" bkcolor="#" normalimage="file='res\min_close.png' source='0,0,16,16'" hotimage="file='res\min_close.png' source='0,16,16,32'" />
        <Control width="7" />
        <Button name="closebtn" tooltip="#close" width="16" height="16" bkcolor="#" normalimage="file='res\min_close.png' source='16,0,32,16'" hotimage="file='res\min_close.png' source='16,16,32,32'" />
      </HorizontalLayout>
    </HorizontalLayout>
    <VerticalLayout>
      <Label height="1" bkcolor="#" />
      <Label height="1" bkcolor="#FF666666" />
      <Label height="1" bkcolor="#FF444444" />
      <Label height="1" bkcolor="#" />
      <HorizontalLayout height="28" inset="2,0,2,0">
        <Option name="options_opt" selected="true" text="#option" font="1" textcolor="#ffaaaaaa" group="opt_tab_group" selectedbkcolor="#ff35383f" normalimage="file='res\tab.png' source='0,0,10,28'" selectedimage="file='res\tab.png' source='0,28,10,56'" />
        <Control width="2" />
        <Option name="serve_opt" text="#serve_setup" font="1" selectedtextcolor="#ffffffff" textcolor="#ffaaaaaa" group="opt_tab_group" normalimage="file='res\tab.png' source='0,0,10,28'" selectedimage="file='res\tab.png' source='0,28,10,56'" />
        <Control width="2" />
        <Option name="AVS_opt" text="#AVS" font="1" selectedtextcolor="#ffffffff" textcolor="#ffaaaaaa" group="opt_tab_group" normalimage="file='res\tab.png' source='0,0,10,28'" selectedimage="file='res\tab.png' source='0,28,10,56'" />
        <Control width="2" />
        <Option name="aboutme_opt" text="#aboutme" font="1" selectedtextcolor="#ffffffff" textcolor="#ffaaaaaa" group="opt_tab_group" normalimage="file='res\tab.png' source='0,0,10,28'" selectedimage="file='res\tab.png' source='0,28,10,56'" />
      </HorizontalLayout>
      <Control height="1" />
      <TabLayout name="main_opt_tab" bkcolor="#ff35383f" selectedid="0">
        <!-- 选项页面 Begin -->
        <VerticalLayout comment="options_opt_body" inset="20,10,20,10">
          <Control />
          <HorizontalLayout height="30" inset="40,0,0,0">
            <CheckBox name="auto_start" text="#auto_start" selected="true" />
            <CheckBox name="min_start" text="#min_start" selected="true" />
          </HorizontalLayout>
          <Control />
          <HorizontalLayout height="30" inset="40,0,0,0">
            <RadioBox name="live" text="#live" selected="true" group="optional_radiobox" />
            <RadioBox name="live_and_videos" text="#live_and_videos" group="optional_radiobox" />
          </HorizontalLayout>
          <Control />
          <HorizontalLayout height="30">
            <Control />
            <Button name="begin_btn" width="70" text="#begin_btn" tooltip="Ctrl+Z" hottextcolor="#ffff0000" normalimage="file='res\btn_bk.png'" bkcolor="#" hotbkcolor="#" />
            <Control />
            <Button name="end_btn" width="70" text="#end_btn" tooltip="Ctrl+X" hottextcolor="#ffff0000" normalimage="file='res\btn_bk.png'" bkcolor="#" hotbkcolor="#" />
            <Control />
          </HorizontalLayout>
          <Control />
          <Label text="#hotkey_explain" align="vcenter" />
        </VerticalLayout>
        <!-- 选项页面 End -->

        <!-- 服务设置页面 Begin -->
        <VerticalLayout comment="serve_opt_body" inset="10,10,10,10">
          <!-- 直播服务 -->
          <HorizontalLayout>
            <VerticalLayout width="110" bkcolor="#ff222222" inset="5,0,5,0">
              <Control />
              <RadioBox name="serve_radio" height="30" text="#live_service" selected="true" group="serve_radiobox" />
              <Control />
            </VerticalLayout>
            <VerticalLayout bkcolor="#ff2a2a2a" inset="10,5,10,5">
              <Control />
              <HorizontalLayout height="25">
                <Label text="#address" width="35" />
                <Container height="25">
                  <ComboBox name="live_address" tooltip="#live_serve_addr" />
                  <Control mouse="false" float="true" pos="-26,8,-15,19" bkimage="file='res\triangle.png'" />
                </Container>
                <Control width="5" />
                <Edit name="live_port_edit" width="50" text="" />
                <Control width="5" />
                <Edit name="live_dir_edit" width="80" text="" />
              </HorizontalLayout>
              <Control />
              <Label name="live_addr_show" height="15" text="访问地址：rtsp://10.18.3.10:55/live123" align="vcenter" />
            </VerticalLayout>
          </HorizontalLayout>
          <Control height="10" />

          <!-- 推流 -->
          <HorizontalLayout>
            <VerticalLayout width="110" bkcolor="#ff222222" inset="5,0,5,0">
              <Control />
              <RadioBox name="push_radio" height="30" text="#push_flow" group="serve_radiobox" />
              <Control />
            </VerticalLayout>
            <VerticalLayout bkcolor="#ff2a2a2a" inset="10,5,10,5">
              <Control />
              <HorizontalLayout height="25">
                <Label text="#address" width="35" />
                <Edit name="push_address" />
                <Control width="5" />
                <Edit name="push_port_edit" width="50" text="" />
                <Control width="5" />
                <Edit name="push_dir_edit" width="80" text="" />
              </HorizontalLayout>
              <Control />
              <Label name="push_addr_show" height="15" text="推送到18.18.8.11   访问地址：rtsp://18.18.8.11:554/live123" align="vcenter" />
            </VerticalLayout>
          </HorizontalLayout>
          <Control height="10" />

          <!-- 录像 -->
          <HorizontalLayout>
            <VerticalLayout width="110" bkcolor="#ff222222" inset="5,0,5,0">
              <Control />
              <CheckBox name="videos_check" height="30" text="#videos" />
              <Control />
            </VerticalLayout>
            <VerticalLayout bkcolor="#ff2a2a2a" inset="10,5,10,5">
              <Control />
              <HorizontalLayout height="25">
                <Label text="#videos" width="35" />
                <Edit name="video_paht_edit" enabled="false" />
                <Control width="5" />
                <Button name="videos_browse_btn" width="50" text="#browse" hottextcolor="#ffff0000" normalimage="file='res\btn_bk.png'" bkcolor="#" hotbkcolor="#" />
              </HorizontalLayout>
              <Control />
            </VerticalLayout>
          </HorizontalLayout>
        </VerticalLayout>
        <!-- 服务设置页面 End -->

        <!-- 音视频页面 Begin -->
        <VerticalLayout comment="AVS_opt_body" inset="10,10,10,10">
          <VerticalLayout bkcolor="#ff222222" inset="10,0,10,0">
            <Control />
            <HorizontalLayout height="25">
              <HorizontalLayout>
                <Label text="#frame_rate" width="30" />
                <Control width="5" />
                <Container height="25">
                  <ComboBox name="frame_rate" >
                    <ListLabelElement text="5" />
                    <ListLabelElement text="10" />
                    <ListLabelElement text="15" />
                    <ListLabelElement text="20" />
                    <ListLabelElement text="25" />
                    <ListLabelElement text="30" />
                  </ComboBox>
                  <Control mouse="false" float="true" pos="-26,8,-15,19" bkimage="file='res\triangle.png'" />
                </Container>
                <Control />
              </HorizontalLayout>
              <HorizontalLayout>
                <Label text="#quality" width="30" />
                <Control width="5" />
                <Container height="25">
                  <ComboBox name="quality">
                    <ListLabelElement text="1" />
                    <ListLabelElement text="2" />
                    <ListLabelElement text="3" />
                    <ListLabelElement text="4" />
                    <ListLabelElement text="5" />
                    <ListLabelElement text="6" />
                    <ListLabelElement text="7" />
                    <ListLabelElement text="8" />
                    <ListLabelElement text="9" />
                    <ListLabelElement text="10" />
                  </ComboBox>
                  <Control mouse="false" float="true" pos="-26,8,-15,19" bkimage="file='res\triangle.png'" />
                </Container>
                <Control />
              </HorizontalLayout>
            </HorizontalLayout>
            <Control />
          </VerticalLayout>
          <Control height="10" />
          <VerticalLayout bkcolor="#ff222222" inset="10,0,10,0">
            <Control />
            <HorizontalLayout height="30">
              <RadioBox name="" width="130" text="#screen" group="AVS_V_radiobox" selected="true" />
              <RadioBox name="" width="130" text="#video" group="AVS_V_radiobox" />
              <Container height="30" inset="0,3,0,2">
                <ComboBox name="" tooltip="#device_list" />
                <Control mouse="false" float="true" pos="-26,11,-15,22" bkimage="file='res\triangle.png'" />
              </Container>
              <Control width="10" />
              <Button name="" width="50" text="#set_up" hottextcolor="#ffff0000" normalimage="file='res\btn_bk.png'" bkcolor="#" hotbkcolor="#" />
            </HorizontalLayout>
            <Control />
          </VerticalLayout>
          <Control height="10" />
          <VerticalLayout bkcolor="#ff222222" inset="10,0,10,0">
            <Control />
            <HorizontalLayout height="30">
              <RadioBox name="" width="130" text="#sound_for_play_and_MIC" group="AVS_A_radiobox" selected="true" />
              <RadioBox name="" width="130" text="#sound_for_MIC" group="AVS_A_radiobox" />
              <Container height="30" inset="0,3,0,2">
                <ComboBox name="" tooltip="#device_list" />
                <Control mouse="false" float="true" pos="-26,11,-15,22" bkimage="file='res\triangle.png'" />
              </Container>
              <Control width="10" />
              <Button name="" width="50" text="#set_up" hottextcolor="#ffff0000" normalimage="file='res\btn_bk.png'" bkcolor="#" hotbkcolor="#" />
            </HorizontalLayout>
            <Control />
          </VerticalLayout>
        </VerticalLayout>
        <!-- 音视频页面 End -->

